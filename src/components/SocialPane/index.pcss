.social-pane {
    display: flex;
    flex-direction: column;
    padding: 0;
    margin: 0;
    list-style: none;

    &__item {
        list-style: none;
        margin-top: 5px;

        &:first-of-type {
            margin-top: 0;
        }

        &:last-of-type {
            margin-bottom: 0;
        }
    }

    &__icon {
        display: block;
        background-size: 40px;
        background-position: center;
        height: 40px;
        width: 40px;
        text-indent: -9999px;
        white-space: nowrap;
        overflow: hidden;

